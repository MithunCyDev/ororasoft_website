{
  "version": 1,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "next build"
      }
    }
  ],
  "routes": [
    {
      "src": "/404",
      "dest": "/not-found"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
